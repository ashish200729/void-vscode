{
  "name": "simple-browser",
  "displayName": "%displayName%",
  "description": "%description%",
  "enabledApiProposals": [
    "externalUriOpener"
  ],
  "version": "1.0.0",
  "icon": "media/icon.png",
  "publisher": "vscode",
  "license": "MIT",
  "aiKey": "0c6ae279ed8443289764825290e4f9e2-1a736e7c-1324-4338-be46-fc2a58ae4d14-7255",
  "engines": {
    "vscode": "^1.70.0"
  },
  "main": "./out/extension",
  "browser": "./dist/browser/extension",
  "categories": [
    "Other"
  ],
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "activationEvents": [
    "onCommand:simpleBrowser.api.open",
    "onOpenExternalUri:http",
    "onOpenExternalUri:https",
    "onWebviewPanel:simpleBrowser.view"
  ],
  "capabilities": {
    "virtualWorkspaces": true,
    "untrustedWorkspaces": {
      "supported": true
    }
  },
  "contributes": {
    "commands": [
      {
        "command": "simpleBrowser.show",
        "title": "Show",
        "category": "Just Browser"
      },
      {
        "command": "simpleBrowser.showAgenticBrowser",
        "title": "Open Agentic Browser",
        "category": "Browser Tools"
      },
      {
        "command": "simpleBrowser.navigate",
        "title": "Navigate to URL",
        "category": "Browser Tools"
      },
      {
        "command": "simpleBrowser.back",
        "title": "Go Back",
        "category": "Browser Tools"
      },
      {
        "command": "simpleBrowser.forward",
        "title": "Go Forward",
        "category": "Browser Tools"
      },
      {
        "command": "simpleBrowser.reload",
        "title": "Reload Page",
        "category": "Browser Tools"
      },
      {
        "command": "simpleBrowser.click",
        "title": "Click Element",
        "category": "Browser Tools"
      },
      {
        "command": "simpleBrowser.type",
        "title": "Type Text",
        "category": "Browser Tools"
      },
      {
        "command": "simpleBrowser.fill",
        "title": "Fill Form Field",
        "category": "Browser Tools"
      },
      {
        "command": "simpleBrowser.press",
        "title": "Press Key",
        "category": "Browser Tools"
      },
      {
        "command": "simpleBrowser.hover",
        "title": "Hover Element",
        "category": "Browser Tools"
      },
      {
        "command": "simpleBrowser.screenshot",
        "title": "Take Screenshot",
        "category": "Browser Tools"
      },
      {
        "command": "simpleBrowser.getContent",
        "title": "Get Page Content",
        "category": "Browser Tools"
      },
      {
        "command": "simpleBrowser.getAccessibilityTree",
        "title": "Get Accessibility Tree",
        "category": "Browser Tools"
      },
      {
        "command": "simpleBrowser.evaluate",
        "title": "Execute JavaScript",
        "category": "Browser Tools"
      },
      {
        "command": "simpleBrowser.waitForSelector",
        "title": "Wait for Element",
        "category": "Browser Tools"
      },
      {
        "command": "simpleBrowser.closeBrowser",
        "title": "Close Browser",
        "category": "Browser Tools"
      },
      {
        "command": "simpleBrowser.hideAgenticBrowser",
        "title": "Hide Agentic Browser",
        "category": "Browser Tools"
      },
      {
        "command": "simpleBrowser.dragAndDrop",
        "title": "Drag and Drop Element",
        "category": "Browser Tools"
      },
      {
        "command": "simpleBrowser.getCookies",
        "title": "Get Cookies",
        "category": "Browser Tools"
      },
      {
        "command": "simpleBrowser.setCookies",
        "title": "Set Cookies",
        "category": "Browser Tools"
      },
      {
        "command": "simpleBrowser.clearCookies",
        "title": "Clear All Cookies",
        "category": "Browser Tools"
      },
      {
        "command": "simpleBrowser.scrollTo",
        "title": "Scroll to Position",
        "category": "Browser Tools"
      },
      {
        "command": "simpleBrowser.scrollBy",
        "title": "Scroll by Offset",
        "category": "Browser Tools"
      },
      {
        "command": "simpleBrowser.scrollIntoView",
        "title": "Scroll Element into View",
        "category": "Browser Tools"
      }
    ],
    "configuration": [
      {
        "title": "Just Browser",
        "properties": {
          "simpleBrowser.focusLockIndicator.enabled": {
            "type": "boolean",
            "default": true,
            "title": "Focus Lock Indicator Enabled",
            "description": "%configuration.focusLockIndicator.enabled.description%"
          },
          "simpleBrowser.useElectronWebview": {
            "type": "boolean",
            "default": false,
            "title": "Use Electron Webview",
            "description": "%configuration.useElectronWebview.description%"
          }
        }
      }
    ]
  },
  "scripts": {
    "compile": "gulp compile-extension:markdown-language-features && npm run build-preview",
    "watch": "npm run build-preview && gulp watch-extension:markdown-language-features",
    "vscode:prepublish": "npm run build-ext && npm run build-preview",
    "build-ext": "node ../../node_modules/gulp/bin/gulp.js --gulpfile ../../build/gulpfile.extensions.js compile-extension:markdown-language-features ./tsconfig.json",
    "build-preview": "node ./esbuild-preview",
    "compile-web": "npx webpack-cli --config extension-browser.webpack.config --mode none",
    "watch-web": "npx webpack-cli --config extension-browser.webpack.config --mode none --watch --info-verbosity verbose"
  },
  "dependencies": {
    "@vscode/extension-telemetry": "^0.9.8"
  },
  "devDependencies": {
    "@types/vscode-webview": "^1.57.0",
    "@vscode/codicons": "^0.0.36"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/vscode.git"
  }
}
